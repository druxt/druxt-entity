{
  "name": "druxt-entity",
  "version": "0.5.1",
  "description": "Provides an Entity render Vue component to be used within a Druxt (DRUpal nuXT) project.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Realityloop/druxt-entity"
  },
  "keywords": [
    "drupal",
    "druxt",
    "entity",
    "entities",
    "field",
    "fields",
    "nuxt"
  ],
  "author": "Stuart Clark <stuart@realityloop.com> (realityloop.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Realityloop/druxt-entity/issues"
  },
  "homepage": "https://github.com/Realityloop/druxt-entity#readme",
  "main": "dist/druxt-entity.ssr.js",
  "module": "dist/druxt-entity.esm.js",
  "unpkg": "dist/druxt-entity.min.js",
  "files": [
    "dist",
    "nuxt"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:es": "cross-env NODE_ENV=production rollup -c --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup -c --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup -c --format iife",
    "dev": "nodemon --ext js,vue --exec 'npm run lint && npm run build && npm run test'",
    "lint": "eslint --ext .js,.vue src",
    "start": "npm run dev",
    "test": "jest"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "druxt-router": "^0.12.0",
    "druxt-schema": "^0.2.0",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.1.0",
    "jest-mock-axios": "^3.2.0",
    "minimist": "^1.2.0",
    "nodemon": "^2.0.2",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
